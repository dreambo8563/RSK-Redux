{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "babel",
    "react",
    "promise"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "__DEV__": false,
    "__TEST__": false,
    "__PROD__": false,
    "__COVERAGE__": false
  },
  "rules": {
    "key-spacing": 0,
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "max-len": [
      2,
      120,
      2
    ],
    "object-curly-spacing": [
      2,
      "always"
    ],
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "no-underscore-dangle": 0,
    "react/jsx-closing-bracket-location": 0,
    "space-before-function-paren": 0,
    "semi": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-curly-spacing": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-indent": 0,
    "react/jsx-equals-spacing": 0,
    "react/jsx-boolean-value": 0,
    "indent": 0,
    "react/no-multi-comp": 0,
    "react/jsx-no-bind":0
  }
}