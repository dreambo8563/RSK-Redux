{
    "name": "react-redux",
    "version": "3.0.0-alpha.2",
    "description": "Get started with React, Redux, and React-Router!",
    "main": "index.js",
    "engines": {
        "node": ">=4.5.0",
        "npm": "^3.0.0"
    },
    "scripts": {
        "clean": "rimraf dist",
        "compile": "better-npm-run compile",
        "lint": "eslint bin build config server src tests",
        "lint:fix": "npm run lint -- --fix",
        "start": "better-npm-run start",
        "dev": "npm run cp-config && better-npm-run dev",
        "test": "npm run cp-config && better-npm-run test",
        "test:dev": "npm run cp-config && npm run test -- --watch",
        "deploy": "better-npm-run deploy",
        "deploy:dev": "better-npm-run deploy:dev",
        "deploy:prod": "better-npm-run deploy:prod",
        "codecov": "cat coverage/*/lcov.info | codecov"
    },
    "betterScripts": {
        "compile": {
            "command": "node bin/compile",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "dev": {
            "command": "nodemon --exec node bin/server.js --ignore dist --ignore coverage --ignore tests --ignore src",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "deploy": {
            "command": "npm run lint  && npm run clean && npm run compile",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "deploy:dev": {
            "command": "npm run deploy",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "deploy:prod": {
            "command": "npm run deploy",
            "env": {
                "NODE_ENV": "production",
                "DEBUG": "app:*"
            }
        },
        "start": {
            "command": "node bin/server",
            "env": {
                "DEBUG": "app:*"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dreambo8563/RSK-Redux.git"
    },
    "author": "vincent",
    "license": "MIT",
    "dependencies": {
        "babel-loader": "^8.0.5",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-es2015": "^6.14.0",
        "babel-preset-react": "^6.11.1",
        "babel-preset-stage-0": "^6.3.13",
        "babel-runtime": "^6.11.6",
        "better-npm-run": "0.1.1",
        "classnames": "^2.2.5",
        "css-loader": "^2.1.0",
        "cssnano": "^4.1.8",
        "debug": "^4.1.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^3.0.1",
        "fs-extra": "^7.0.1",
        "html-webpack-plugin": "^3.2.0",
        "imports-loader": "^0.8.0",
        "ip": "^1.1.2",
        "json-loader": "^0.5.4",
        "normalize.css": "^8.0.1",
        "postcss-loader": "^3.0.0",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-redux": "^6.0.0",
        "react-router": "^4.3.1",
        "redux": "^4.0.1",
        "redux-thunk": "^2.0.0",
        "request": "^2.75.0",
        "rimraf": "^2.5.4",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.0",
        "whatwg-fetch": "^3.0.0",
        "yargs": "^12.0.5"
    },
    "devDependencies": {
        "autoprefixer": "^9.4.6",
        "babel-eslint": "^10.0.1",
        "babel-plugin-istanbul": "^5.1.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "chai-as-promised": "^7.1.1",
        "chai-enzyme": "^0.8.0",
        "cheerio": "^0.22.0",
        "codecov": "^3.1.0",
        "connect-history-api-fallback": "^1.3.0",
        "enzyme": "^3.8.0",
        "eslint": "^5.12.1",
        "eslint-config-standard": "^12.0.0",
        "eslint-config-standard-react": "^7.0.2",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-promise": "^4.0.1",
